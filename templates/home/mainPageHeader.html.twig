<header class="section-header">
    <a href="{{ path('homepage') }}" class="section-header__logo text-fsdd__bold">
        <span>A</span> <span class="blue">R</span> <span>T</span> <span>I</span> <span>C</span>
    </a>
    <nav class="section-header__center">
        {# nav #}
        <ul class="section-header__nav dn" data-header-menu-nav>
            <li class="nav__item">
                <a href="" class="link text-fsb__semibold">About project</a>
            </li>
            <li class="nav__item">
                <a href="" class="link text-fsb__semibold">For advertisers</a>
            </li>
            <li class="nav__item">
                <a href="" class="link text-fsb__semibold">Feedback</a>
            </li>
            <li class="nav__item">
                <a href="" class="link text-fsb__semibold">Contacts</a>
            </li>
        </ul>
        {# form #}
        <form action="{{ path('searchPerformers') }}" method="POST" class="section-header__search opened" data-header-menu-form>
            {# <div class="search__wrapper">
                <svg width="24" height="24" viewBox="0 0 24 24" fill="none" class="svg">
                    <path d="M14.4121 14.4121L20 20" stroke="#035FFE" stroke-width="2" stroke-linecap="round"/>
                    <path d="M10 16C13.3137 16 16 13.3137 16 10C16 6.68629 13.3137 4 10 4C6.68629 4 4 6.68629 4 10C4 13.3137 6.68629 16 10 16Z" stroke="#035FFE" stroke-width="2" stroke-linecap="square"/>
                </svg>
                <input type="text" class="search__input text-fsd__regular" name="performer" placeholder="What do you want find?">
                <button type="submit" class="search__btnn text-fsd__bold">Search</button>
            </div> #}
        </form>
    </nav>
    <div class="section-header__right">
        {# singup and singin logic #}
        {% if app.user %}
            {% if app.user.roles|first == 'ROLE_USER' %}
                <a href="" class="section-header__bell">
                    <svg width="24" height="24" viewBox="0 0 24 24" fill="none">
                        <path d="M18 9C18 10.6667 18 12.3333 18 14C18 16 18.6667 17.3333 20 18H4C5.33333 17.3333 6 16 6 14C6 12.3333 6 10.6667 6 9C6 5.68629 8.68629 3 12 3C15.3137 3 18 5.68629 18 9Z" stroke="#263238" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"/>
                        <path d="M10 18C10 19.1046 10.8954 20 12 20C13.1046 20 14 19.1046 14 18" stroke="#263238" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"/>
                    </svg>
                </a>
                <a href="" class="section-header__mail">
                    <svg width="24" height="24" viewBox="0 0 24 24" fill="none">
                        <path d="M22 5H2V19H22V5Z" stroke="#263238" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"/>
                        <path d="M2 5L12 14L22 5" stroke="#263238" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"/>
                        <path d="M2 19.0002L8.825 11.2002" stroke="#263238" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"/>
                        <path d="M22 19.0002L15.156 11.1782" stroke="#263238" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"/>
                    </svg>
                    <div class="mail-count">12</div>
                </a>
                <div class="section-header__singup">
                    <span class="text-fsb__semibold">My cabinet</span>
                    <div class="user-img">
                        <img src="{{ asset('img/user.png') }}" class="img">
                    </div>
                </div>
                {# <div class="dropdown">
                    <a href="{{ path('user_settings') }}">Profile</a>
                    <a href="{{ path('user_profile') }}">Orders</a>
                    <a href="{{ path('app_logout') }}">Logout</a>
                </div> #}
            {% elseif app.user.roles|first == 'ROLE_PERFORMER' %}
                <a href="" class="section-header__bell">
                    <svg width="24" height="24" viewBox="0 0 24 24" fill="none">
                        <path d="M18 9C18 10.6667 18 12.3333 18 14C18 16 18.6667 17.3333 20 18H4C5.33333 17.3333 6 16 6 14C6 12.3333 6 10.6667 6 9C6 5.68629 8.68629 3 12 3C15.3137 3 18 5.68629 18 9Z" stroke="#263238" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"/>
