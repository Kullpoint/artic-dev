                    <svg width="24" height="24" viewBox="0 0 24 24" fill="none">
                        <path d="M22 5H2V19H22V5Z" stroke="#263238" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"/>
                        <path d="M2 5L12 14L22 5" stroke="#263238" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"/>
                        <path d="M2 19.0002L8.825 11.2002" stroke="#263238" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"/>
                        <path d="M22 19.0002L15.156 11.1782" stroke="#263238" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"/>
                    </svg>
                    <div class="mail-count">12</div>
                </a>
                <div class="section-header__singup">
                    <span class="text-fsb__semibold">My cabinet</span>
                    <div class="user-img">
                        {% if app.user.performer.avatar %}
                            <img src="{{ asset('uploads/avatars/' ~ app.user.performer.avatar) }}" class="img">
                        {% else %}
                            <img src="{{ asset('img/user.png') }}" class="img">
                        {% endif %}
                    </div>
                </div>
                {# <div class="dropdown">
                    <a href="{{ path('user_settings') }}">Profile</a>
                    <a href="{{ path('performer_profile') }}">Orders</a>
                    <a href="{{ path('performer_transactions') }}">Transactions</a>
                    <a href="{{ path('app_logout') }}">Logout</a>
                </div> #}
            {% elseif app.user.roles|first == 'ROLE_PARTNER' %}
                <a href="" class="section-header__bell">
                    <svg width="24" height="24" viewBox="0 0 24 24" fill="none">
                        <path d="M18 9C18 10.6667 18 12.3333 18 14C18 16 18.6667 17.3333 20 18H4C5.33333 17.3333 6 16 6 14C6 12.3333 6 10.6667 6 9C6 5.68629 8.68629 3 12 3C15.3137 3 18 5.68629 18 9Z" stroke="#263238" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"/>
