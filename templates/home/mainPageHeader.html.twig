        {# languages #}
        <div class="section-header__languages" data-languages-btn>
            <svg width="24" height="24" viewBox="0 0 24 24" fill="none" class="lang-svg">
                <path d="M12 22C17.5228 22 22 17.5228 22 12C22 6.47715 17.5228 2 12 2C6.47715 2 2 6.47715 2 12C2 17.5228 6.47715 22 12 22Z" stroke="#035FFE" stroke-width="2" stroke-linecap="square"/>
                <path d="M12 22C14.6667 19.5758 16 16.2424 16 12C16 7.75758 14.6667 4.42424 12 2C9.33333 4.42424 8 7.75758 8 12C8 16.2424 9.33333 19.5758 12 22Z" stroke="#035FFE" stroke-width="2" stroke-linecap="round"/>
                <path d="M2.5 15H21.5M2.5 9H21.5H2.5Z" stroke="#035FFE" stroke-width="2" stroke-linecap="round"/>
            </svg>
        </div>
        <ul class="language-list dn" data-language-list>
            <li class="language-list__item">
                <img src="{{ asset('img/NEW/ua.svg') }}" class="svg">
                <span class="text-fsd__regular">Укр</span>
            </li>
            <li class="language-list__item">
                <img src="{{ asset('img/NEW/ru.svg') }}" class="svg">
                <span class="text-fsd__regular">Рус</span>
            </li>
            <li class="language-list__item">
                <img src="{{ asset('img/NEW/en.svg') }}" class="svg">
                <span class="text-fsd__regular">Eng</span>
            </li>
        </ul>
        {# menu #}
        <div class="section-header__menu" data-header-menu-btn>
            <svg width="24" height="24" viewBox="0 0 24 24" fill="none" class="menu-svg">
                <path d="M10 10H18M6 6H18M6 14H18M10 18H18" stroke="#035FFE" stroke-width="2" stroke-linecap="square"/>
            </svg>
            <svg width="24" height="24" viewBox="0 0 24 24" fill="none" class="menu-close-svg">
