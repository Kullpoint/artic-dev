        {% else %}
            <a class="section-header__login text-fsb__semibold" href="{{ path('app_login') }}">Sign in</a>
            <a class="section-header__singup" href="{{ path('app_register') }}">
                <span class="text-fsb__semibold">Sign up</span>
                <svg width="24" height="24" viewBox="0 0 24 24" fill="none" class="user-svg">
